---
import Social from "./Social.astro";
const platFormUserNameMap: string[][] = [
    ["github", "justDabuK"],
    ["instagram", "justdabu"],
    ["reddit", "justDabuK"]
]
---

<style>
    footer {
        display: flex;
        gap: 1rem;
        margin-top: 2rem;
    }
</style>

<footer>
        {platFormUserNameMap.map((platFormAndUserName) => <Social platform={platFormAndUserName[0]} username={platFormAndUserName[1]} />)}
</footer>